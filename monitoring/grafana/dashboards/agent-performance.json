{
  "dashboard": {
    "title": "Agent Performance Dashboard",
    "tags": ["agentic-ai", "agents"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Tasks by Agent",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum by (agent_type) (agent_tasks_total)",
            "legendFormat": "{{agent_type}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Agent Success Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(agent_tasks_successful_total) / sum(agent_tasks_total) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Average Task Duration by Agent",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "avg by (agent_type) (agent_task_duration_seconds)",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 4,
        "title": "Task Status Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum by (status) (agent_tasks_by_status)",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "id": 5,
        "title": "LLM API Calls by Agent",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum by (agent_type) (agent_llm_calls_total)",
            "legendFormat": "{{agent_type}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Recent Errors",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "agent_errors_total",
            "format": "table",
            "instant": true
          }
        ]
      }
    ],
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 1
  }
}
